  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSourceForPortal</DataSourceName>
        <CommandText>select dp.Product_Name, dsc.name SubCatName, dpc.Name as CatName,
		 dc.AlternateCustomerID,  dc.FirstName, dc.LastName, dc.Region, dc.State, dc.City,
		 dd.Month, dd.MonthName, dd.Year,
		 fs.LineTotal, fs.OrderQty,  fs.OrderID, fs.SubTotal
from FactSales fs inner join DimProduct dp on fs.ProductID = dp.ProductID 
		inner join DimSubCategory dsc on dp.ProductSubCategoryID = dsc.ProductSubcategoryID
		inner join DimCategory dpc on dsc.ProductCategoryID = dpc.ProductCategoryID
		inner join DimCustomer dc on fs.CustomerID = dc.CustomerID
		inner join DimDate dd on fs.OrderDateKey = dd.DateKey</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
